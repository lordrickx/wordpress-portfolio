{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 2,
    "name": "acrobits/menu-item-basic",
    "version": "0.1.1",
    "title": "Menu Item Simple",
    "category": "widgets",
    "description": "A simple menu item with title and link",
    "supports": {
        "html": false
    },
    "attributes": {
        "title": {
            "type": "string",
            "default": "Menu Item"
        },
        "url": {
            "type": "string",
            "default": "#"
        },
        "linkText": {
            "type": "string",
            "default": "Click here"
        }
    },
    "textdomain": "menu-item-basic",
    "editorScript": "file:./index.js",
    "editorStyle": "file:./index.css",
    "style": "file:./style-index.css"
} 